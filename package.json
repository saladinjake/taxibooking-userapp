  "main": "./server/dynamic_server/server.js",
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    },
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-runtime",
      [
        "istanbul",
        {
          "exclude": [
            "**/tests/**/*.tester.js"
          ]
        }
      ]
    ]
  },
  "nyc": {
    "exclude": [
      "UI/",
      "server/dynamic_server/dummy_api/**",
      "server/dynamic_server/config/",
      "server/dynamic_server/dummy_api/tests",
      "server/dynamic_server/mongo_api/tests",
      "server/dynamic_server/postgres_api/tests",
      "server/dynamic_server/sequelize_api/tests",
      "server/dynamic_server/mongo_api/models/db",
      "server/dynamic_server/postgres_api/models/db",
      "server/dynamic_server/postgres_api/services/**",
      "server/dynamic_server/postgres_api/helpers/**",
      "server/dynamic_server/postgres_api/middlewares/**",
      "helpers/**",
      "server/dynamic_server/sequelize_api/models/db",
      "dist/",
      "server/dynamic_server/server.js",
      "server/static_server/server.js"
    ],
    "include": [
      "**/server"
    ]
  },
  "scripts": {
    "start:gulp:devmode": "gulp watch",
    "babel-node": "babel-node --presets=@babel/preset-env",
    "heroku-postbuild": "npm run build",
    "lint": "eslint source/ --quiet",
    "lint:write": "eslint --debug . --fix",
    "prettier": "prettier --write frontend/**/*.jsx",
    "start:server":"babel-node --presets=@babel/preset-env server.js",
    "start-react": "node server_react.js",
    "webpack": "webpack --config ./webpack.config.prod.js",
    "start:dev": "webpack-dev-server --config ./webpack.config.dev.js --open --hot --port 4000",
    "start": "node server_react.js",
    "deploy": "now public",
    "build": "webpack -p --config ./webpack.config.prod.js",
    "clean-react": "del-cli  dist-reactive ",
    "clean": "del-cli  buildfolder/**/*.js ",
    "clean-front": "del-cli  dist-reactive/*",
    "clean-front-admin": "del-cli  dist-reactive-admin/*",
    "build-css": "node-sass scss/app.scss public/css/app.css",
    "build-for-production": "npm run clean && npm run build-server  && webpack -p --config ./webpackuploadtoserver.config.js",
    "validate": "npm run lint; npm run test && npm outdated --depth 0",
    "dbrun-migrations-mongoose-deploy": "npm run migration-mongoose-seed-deploy",
    "devups-upload": "webpack -p --config ./webpackuploadtoserver.config.js",
    "build-front": "npm run clean-front && npm run webpack",
    "start-front-buildfolder": "export NODE_ENV=DEVELOPMENT || set NODE_ENV=DEVELOPMENT &&  node server_react.js"
  },
